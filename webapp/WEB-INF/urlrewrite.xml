<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
  <rule stopProcessing="true">
    <from>^/new$</from>
    <to>/new</to>
  </rule>
  <rule stopProcessing="true">
    <from>^/set-username$</from>
    <to>/set-username</to>
  </rule>
  <rule stopProcessing="true">
    <from>^/index$</from>
    <to>/index</to>
  </rule>

  <rule>
    <from>^/(\w+)/(\w+)$</from>
    <to>/project/$1/$2</to>
  </rule>

  <rule>
    <from>^/(\w+)$</from>
    <to>/user/$1</to>
  </rule>

  <rule>
    <from>^/$</from>
    <to>/index</to>
  </rule>
</urlrewrite>
