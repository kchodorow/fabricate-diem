<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>
  <rule>
    <from>^/new$</from>
    <to>/+/new</to>
  </rule>
  <rule>
    <from>^/set-username$</from>
    <to>/+/set-username</to>
  </rule>
  <rule>
    <from>^/index$</from>
    <to>/+/index</to>
  </rule>

  <rule>
    <from>^/([A-Za-z]\w*)/([-A-Za-z_0-9]+)$</from>
    <to>/project/$1/$2</to>
  </rule>

  <rule>
    <from>^/([A-Za-z]\w*)$</from>
    <to>/user/$1</to>
  </rule>

  <rule>
    <from>^/$</from>
    <to>/+/index</to>
  </rule>
</urlrewrite>
